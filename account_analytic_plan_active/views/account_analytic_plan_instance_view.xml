<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="account_analytic_plan_instance_view_form" model="ir.ui.view">
            <field name="name">Inherit : Account Analytic Plan Instance Active Form </field>
            <field name="model">account.analytic.plan.instance</field>
            <field name="inherit_id" ref="account_analytic_plans.account_analytic_plan_instance_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after">
                    <field name="active"/>
                </xpath>
           </field>
        </record>
        <record id="account_analytic_plan_instance_view_tree" model="ir.ui.view">
            <field name="name">Inherit : Account Analytic Plan Instance Active Tree </field>
            <field name="model">account.analytic.plan.instance</field>
            <field name="inherit_id" ref="account_analytic_plans.account_analytic_plan_instance_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after">
                    <field name="active"/>
                </xpath>
           </field>
        </record>
    </data>
</openerp>
